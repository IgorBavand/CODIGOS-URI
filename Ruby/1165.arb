n = gets.chomp.to_i

i = 1
while i <= n  do
    numero = gets.chomp.to_i
    cont = 0
    l = 2
    while l < numero do
        if numero%l == 0
            cont = cont + 1
        end
        l = l+1
    end

    if cont == 0
        puts("#{numero} eh primo")
    else
        puts("#{numero} nao eh primo")
    end

    i = i+1
end